{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "CookieAuthOptions": {
    "Name": "SchoolWebSessionId",
    "LoginPath": "/Auth/Login",
    "LogOutPath": "/Auth/LogOut",
    "AccessDeniedPath": "/Auth/Err",
    "SlidingExpiration": true,
    "TimeOut": 3600 
  } 


}
